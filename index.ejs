<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="google" value="notranslate">
    <meta name="description" content="DinosaurMod is a Mod of Penguinmod that adds more blocks, extensions and new features." />
    <title><%= htmlWebpackPlugin.options.title %></title>
    <link rel="apple-touch-icon" href="<%= htmlWebpackPlugin.options.root %>images/apple-touch-icon.png">
    <link rel="icon" type="image/x-icon" href="https://dinosaurmod.github.io/favicon.ico">
    <% if (htmlWebpackPlugin.options.root === "/" || htmlWebpackPlugin.options.root === "") { %>
      <link rel="manifest" href="<%= htmlWebpackPlugin.options.root %>manifest.webmanifest">
    <% } %>
    <style>
      #splash {
        position: absolute; width: 100%; height: 100%; top: 0; left: 0;
        display: flex; align-items: center; justify-content: center;
        font-family: "Helvetica Neue", Helvetica, Arial, sans-serif; color: #76fa02;
      }
      .tw-loaded #splash, #splash-content { display: none; }
      #splash[splash-theme="dark"] { background-color: #333; color: #fff; }
      #splash-spinner:after {
        content: " "; display: block; width: 64px; height: 64px;
        border-radius: 50%; border: 6px solid; border-color: currentColor transparent currentColor transparent;
        animation: splash-spinner 1.2s linear infinite;
      }
      @keyframes splash-spinner {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(360deg); }
      }
    </style>
  </head>
  <body>
    <div id="splash" aria-hidden="true">
      <noscript>
        <h1>Enable JavaScript</h1>
      </noscript>
      <div id="splash-content">
        <div id="splash-spinner"></div>
        <p id="splash-debug-error"></p>
     </div>
    </div>
    <script>
      (async function() {
        document.querySelector('#splash-content').style.display = 'block';
        const errorLabel = document.querySelector('#splash-debug-error');
        window.addEventListener('error', function(event) {
          const errorDetails = {
            message: event.message,
            filename: event.filename,
            lineno: event.lineno,
            colno: event.colno,
            error: event.error ? event.error.stack || event.error.toString() : 'No error object'
          };
          if (errorLabel) errorLabel.innerHTML += `<pre>${JSON.stringify(errorDetails, null, 2)}</pre>`;
        });
        <% if (!htmlWebpackPlugin.options.noTheme) { %>
        try {var localTheme = localStorage.getItem('tw:theme');} catch (e) {}
        if (localStorage.getItem("tw:accent:customAccents") == null) localStorage.setItem("tw:accent:customAccents", JSON.stringify([]));
        if (localTheme ? localTheme === 'dark' : window.matchMedia('(prefers-color-scheme: dark)').matches) document.querySelector('#splash').setAttribute('splash-theme', 'dark');
        try {var accentTheme = localStorage.getItem('tw:accent');} catch (e) {}
        if (accentTheme) document.body.setAttribute("coloraccent", accentTheme || "Lime Green")
        if (accentTheme == null) document.body.setAttribute("coloraccent", "Lime Green")
        window.Recolor = {primary: getComputedStyle(document.body).getPropertyValue('--motion-primary')}
        if (new URLSearchParams(location.search).has("666")) {document.body.setAttribute("colorAccent", "OOHVERYSCARY"); document.body.setAttribute('theme', 'dark'); document.querySelector('#splash').setAttribute('splash-theme', 'dark');
          while (document.querySelector('#app') == null) {await new Promise(r => setTimeout(r, 50))}
          document.querySelector('#app').style.filter = "contrast(200%)";
          const evt = new Event('click');
          document.dispatchEvent(evt);
          while (true) {
            const thisAudio = new Audio('https://dinosaurmod.github.io/this.mp3');
            const waittillthis = Math.floor(Math.random() * (2500 * 100)) + 1
            const thise = document.createElement("div");
            thise.style.display = "none";
            thise.innerHTML = String(waittillthis)
            thise.className = "secret"
            document.body.appendChild(thise)
            await new Promise(r => setTimeout(r, waittillthis))
            thisAudio.play()
            document.body.removeChild(thise)
          }
        }
        <% } %>
      })();
    </script>
    <div id="app"></div>
  </body>
</html>
